{
  "id" : "patient-mapping",
  "url" : "https://datatools4heart.eu/fhir/mappings/kuh/patient-mapping",
  "name" : "patient-mapping",
  "title" : "Patient table mapping",
  "isDraft" : false,
  "source" : [ {
    "alias" : "source",
    "url" : "https://datatools4heart.eu/fhir/schemas/kuh-chd/Patient",
    "joinOn" : [ ]
  } ],
  "context" : { },
  "variable" : [ ],
  "mapping" : [ {
    "expression" : {
      "name" : "result",
      "language" : "application/fhir-template+json",
      "value" : {
        "id" : "{{mpp:getHashedId('Patient', identifier.toString())}}",
        "meta" : {
          "profile" : [ "https://datatools4heart.eu/fhir/StructureDefinition/HFR-Patient" ]
        },
        "resourceType" : "Patient",
        "gender" : "{{gender}}",
        "birthDate" : "{{birthDate.utl:toFhirDate('yyyy-MM')}}",
        "identifier" : [ {
          "value" : "{{identifier.toString()}}"
        } ]
      }
    }
  } ]
}