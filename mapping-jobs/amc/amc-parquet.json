    "name" : "ecg-mapping",
    "mappingRef" : "https://datatools4heart.eu/fhir/mappings/amc/ecg-mapping",
    "sourceBinding" : {
      "source" : {
        "jsonClass" : "FileSystemSource",
        "path" : "ECG.parquet",
        "contentType" : "parquet",
        "options" : {
          "distinct" : "true"
        },
        "preprocessSql" : "select count(*) as count, Pseudo_id, EcgMetingId,\r\narray_agg(EcgAfnameDatumTijd) as EcgAfnameDatumTijd,\r\narray_agg(ValueQuantity) as ValueQuantity,\r\narray_agg(LOINCcode) as LOINCcode,\r\narray_agg(LOINCdisplay) as LOINCdisplay\r\nfrom source \r\nwhere ValueQuantity != 0 AND NOT (LOINCcode = '8633-0' AND ValueQuantity > 300)\r\ngroup by Pseudo_id, EcgMetingId",
        "sourceRef" : "amc-source"
      },
