    }
  }, {
    "name" : "med-prescription-mapping",
    "mappingRef" : "https://datatools4heart.eu/fhir/mappings/amc/med-prescription-mapping",
    "sourceBinding" : {
      "source" : {
        "jsonClass" : "FileSystemSource",
        "path" : "MedicationPrescription.parquet",
        "contentType" : "parquet",
        "options" : {
          "datetimeRebaseMode" : "CORRECTED"
        },
        "preprocessSql" : "select * \nfrom source\nwhere ATCCode IS NOT NULL and StartDate IS NOT NULL and StartDate > '1900-01-01'",
        "sourceRef" : "amc-source"
      },
      "encounters" : {
        "jsonClass" : "FileSystemSource",
        "path" : "Encounters.parquet",
        "contentType" : "parquet",
        "options" : { },
        "preprocessSql" : "select * \r\nfrom encounters\r\nwhere EncounterClassCode = 'IMP'",
        "sourceRef" : "amc-source"
      }
    }
  }, {
    "name" : "nyha-mapping",
    "mappingRef" : "https://datatools4heart.eu/fhir/mappings/amc/nyha-mapping",
