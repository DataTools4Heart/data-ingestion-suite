        "path" : "Procedure.csv",
        "contentType" : "csv",
        "options" : { },
        "sourceRef" : "file-system-csv"
      }
    }
  }, {
    "name" : "med-prescription-mapping",
    "mappingRef" : "https://datatools4heart.eu/fhir/mappings/amc/med-prescription-mapping",
    "sourceBinding" : {
      "source" : {
        "jsonClass" : "FileSystemSource",
        "path" : "MedicationPrescription.csv",
        "contentType" : "csv",
        "options" : { },
        "preprocessSql" : "select * \nfrom source\nwhere ATCCode IS NOT NULL and StartDate IS NOT NULL",
        "sourceRef" : "file-system-csv"
      }
    }
  } ],
  "dataProcessingSettings" : {
    "saveErroneousRecords" : false,
    "archiveMode" : "off"
  },
  "useFhirSinkAsIdentityService" : false
}